# U+00C5:   LATIN CAPITAL LETTER A WITH RING ABOVE
push @{$codesets{"Lat15"}}, "U+00C6";
# U+00C6:   LATIN CAPITAL LETTER AE
push @{$codesets{"Lat15"}}, "U+00C7";
# U+00C7:   LATIN CAPITAL LETTER C WITH CEDILLA
