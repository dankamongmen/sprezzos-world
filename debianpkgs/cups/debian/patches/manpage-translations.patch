+
+# type: Plain text
+#: classes.conf.man:112
+msgid ""
+"I<cupsd(8)>, I<cupsd.conf(5)>, I<mime.convs(5)>, I<mime.types(5)>, "
+"I<printers.conf(5)>,"
+msgstr ""
