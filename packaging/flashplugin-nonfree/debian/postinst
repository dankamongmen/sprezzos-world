#!/bin/sh

set -e

case "$1" in
    configure)
	rm -f /var/cache/flashplugin-nonfree/get-upstream-version.pl
	update-flashplugin-nonfree --install --fast || true
    ;;

    abort-upgrade|abort-remove|abort-deconfigure)
    ;;

    *)
        echo "postinst called with unknown argument \`$1'" >&2
        exit 1
    ;;
esac

#DEBHELPER#

exit 0


