+void
+Data_Start(char *initializer_fct)
+{
+  /* last routine has to be closed first */
+  if (act_routine[0] != '\0')
+    {
+      Inst_Printf("jr", "$31");
+      Inst_Printf(".end", "%s", act_routine);
+
+      act_routine[0] = '\0';
+    }
+
+  if (initializer_fct == NULL)
+    return;
+
+  Inst_Printf(".section", ".ctors,\"aw\",@progbits");
+  Inst_Printf(".word", "%s", initializer_fct);
+}
