-              "name": "installPrefix",
-              "desc": "<p>A compiled-in property that exposes <code>NODE_PREFIX</code>.\n\n</p>\n<pre><code>console.log(&apos;Prefix: &apos; + process.installPrefix);</code></pre>\n"
-            },
