+{
+  /* we are printing the fixed doubles at the end of the file,
+   * they will appear in the data section */
+  if (dbl_arg_buffer[0] != '\0')
+    {
+      Label_Printf(".section\t.rodata");
+      Label_Printf(dbl_arg_buffer);
+      dbl_arg_buffer[0] = '\0';
+    }
