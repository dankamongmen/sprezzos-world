+      /* Fill in the values we have not gotten from the kernel through the
+	 auxiliary vector.  */
+#  undef SEE
+#  define SEE(UID, var, uid) \
+   var ^= __get##uid ()
+      SEE (UID, uid, uid);
+      SEE (EUID, uid, euid);
+      SEE (GID, gid, gid);
+      SEE (EGID, gid, egid);
+
+      /* If one of the two pairs of IDs does not match this is a setuid
+	 or setgid run.  */
+      INTUSE(__libc_enable_secure) = uid | gid;
+    }
