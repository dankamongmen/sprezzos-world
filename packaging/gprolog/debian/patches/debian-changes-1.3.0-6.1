+/*-------------------------------------------------------------------------*
+ * INLINED CODE                                                            *
+ *                                                                         *
+ *-------------------------------------------------------------------------*/
+/* all %s will be replaced with the function's name
+ * all %d will be replaced with the current nb_inlines
+ */
+static long nb_inlines = 0;
+static char *def_inlines[] = {
+  /* name            code */
+  0, 0				/* end of list */
+};
+
