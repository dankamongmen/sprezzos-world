msgid "B<addgroup> --system [options] [--gid ID] group"
msgstr "B<addgroup> --system [options] [--gid ID] groupe"

# type: Plain text
#. type: Plain text
#: ../adduser.8:22
msgid "B<adduser> [options] user group"
msgstr "B<adduser> [options] utilisateur groupe"

