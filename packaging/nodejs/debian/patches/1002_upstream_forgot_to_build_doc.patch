+                }
+              ]
+            }
+          ]
+        },
+        {
+          "textRaw": "clearTimeout(timeoutId)",
+          "type": "method",
+          "name": "clearTimeout",
+          "desc": "<p>Prevents a timeout from triggering.\n\n</p>\n",
+          "signatures": [
