+	      file = kf->kf_path;
+	      break;
+	    }
+	  kf_bufp += kf->kf_structsize;
+	}
+
+      if (kf_bufp >= kf_buf + kf_len)
+	{
+	  __set_errno (EBADF);
+	  return -1;
+	}
+
