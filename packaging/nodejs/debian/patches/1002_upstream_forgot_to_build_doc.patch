+            {
+              "textRaw": "request.trailers",
+              "name": "trailers",
+              "desc": "<p>Read only; HTTP trailers (if present). Only populated after the &apos;end&apos; event.\n\n</p>\n"
+            },
+            {
+              "textRaw": "request.httpVersion",
+              "name": "httpVersion",
